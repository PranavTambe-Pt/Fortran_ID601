program cat
    implicit NONE
    ! print "(a10,i1)", "ABS(-1) = ", ABS(-1)
    ! Absolute Value
    print "(a10,i1)", "abs(-1) ",abs(-1)
    ! Sqrt
    print "(a11,f3.1)", "sqrt(81.0)     ",sqrt(81.0)
    ! exp 
    print *,exp(1.0)
    ! log
    print*,log(2.71)
    !  int
    print *,int(2.999)
    ! nint
    print *,nint(2.49)
    ! floor
    print *,floor(2.9999)
    !  max
    print *, min(3.2,4.5,6.7,1.0)
    !  min
    print *, max(5.6,4.5)
    ! sin 
    print *, sin(1.57)
    ! cos 
    print *, cos(1.57)
    ! tan 
    print *,tan(1.57)





end program cat